  },\n    // 删除数据\n    deleteData(index) {\n      axios.delete(`http://localhost:3030/fruits/${index}`).then( () => {\n        this.getFruitList();\n      })\n    }\n  },\n  created() {\n    this.getFruitList();\n  }\n}\n</script>"]}]}